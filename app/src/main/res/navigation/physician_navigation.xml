<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/mobile_navigation"
    app:startDestination="@+id/navigation_home">

    <fragment
        android:id="@+id/navigation_home"
        android:name="com.example.healthadvisor.ui.Post.PostFragment"
        android:label="@string/title_post"
        tools:layout="@layout/fragment_post" />

    <fragment
        android:id="@+id/navigation_dashboard"
        android:name="com.example.healthadvisor.ui.dr_appointment.DrAppointmentFragment"
        android:label="@string/title_appointment"
        tools:layout="@layout/fragment_appointment_dr" />

    <fragment
        android:id="@+id/navigation_notifications"
        android:name="com.example.healthadvisor.ui.dr_chat.DrChatsFragment"
        android:label="@string/title_chats"
